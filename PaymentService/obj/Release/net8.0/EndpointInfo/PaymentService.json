  "components": {
    "schemas": {
      "BillDTO": {
        "type": "object",
        "properties": {
          "billId": {
            "type": "integer",
            "format": "int32"
          },
          "quantity": {
            "type": "integer",
            "format": "int32"
          },
          "price": {
            "type": "number",
            "format": "double"
          },
          "taxes": {
            "type": "number",
            "format": "double"
          },
          "date": {
            "type": "string",
            "format": "date-time"
          },
          "service": {
            "type": "string",
            "nullable": true
          },
          "unit": {
            "type": "string",
            "nullable": true
          },
          "paymentId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "Guest": {
        "required": [
          "company",
          "email",
          "gender",
          "name",
          "phoneNumber"
        ],
        "type": "object",
        "properties": {
          "guestId": {
            "type": "integer",
            "format": "int32"
          },
          "phoneNumber": {
            "minLength": 1,
            "pattern": "^\\+91\\s[7-9]\\d{9}$",
            "type": "string",
            "format": "tel"
          },
          "company": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "name": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "gender": {
            "maxLength": 10,
            "minLength": 0,
            "type": "string"
